{
  "kind": "collectionType",
  "collectionName": "vendors",
  "info": {
    "singularName": "vendor",
    "pluralName": "vendors",
    "displayName": "Vendor"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "brandImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "description": {
      "type": "text"
    },
    "books": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::book.book"
    }
  }
}
